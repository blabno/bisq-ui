{
  "name": "bisq-ui",
  "version": "0.0.1",
  "author": "Labnoratory",
  "private": true,
  "main": "electron/electron.js",
  "scripts": {
    "start": "node server.js",
    "serve-all": "nf start",
    "serve-ionic": "ionic-app-scripts serve",
    "serve-electron": "electron .",
    "build-ionic": "ionic cordova build android",
    "build-electron": "npm run build && node_modules/.bin/build",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "extract": "ngx-translate-extract -i ./src/app -o ./src/assets/i18n/en.json ./src/assets/i18n/pl.json --clean --sort --format namespaced-json -m t",
    "e2e": "webdriver-manager update && protractor ./e2e/conf.js",
    "e2e-backend": "node ./e2e/server.js"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@angular/router": "5.0.3",
    "@fortawesome/fontawesome-free-webfonts": "1.0.4",
    "@ionic-native/clipboard": "4.6.0",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/pro": "1.0.20",
    "@ionic/storage": "2.1.3",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "bluebird": "3.5.1",
    "bootstrap": "4.0.0",
    "cordova-android": "6.3.0",
    "cordova-clipboard": "1.1.1",
    "cordova-plugin-device": "2.0.1",
    "cordova-plugin-ionic-keyboard": "2.0.5",
    "cordova-plugin-ionic-webview": "1.1.16",
    "cordova-plugin-splashscreen": "5.0.2",
    "cordova-plugin-whitelist": "1.3.3",
    "express": "4.16.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "jquery": "3.3.1",
    "lodash": "4.17.5",
    "md5": "2.2.1",
    "moment": "2.21.0",
    "ngx-qrcode2": "0.1.0",
    "path": "0.12.7",
    "popper.js": "1.12.9",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "config": {
    "ionic_bundler": "webpack",
    "ionic_copy": "./config/copy.config.js",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "devDependencies": {
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ionic/app-scripts": "3.1.8",
    "body-parser": "1.18.2",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "cors": "2.8.4",
    "electron": "1.8.3",
    "electron-builder": "20.2.1",
    "foreman": "2.0.0",
    "mocha": "5.1.0",
    "protractor": "5.3.1",
    "request": "2.85.0",
    "request-promise": "4.2.2",
    "typescript": "2.4.2"
  },
  "build": {
    "appId": "org.labnoratory.bisqui",
    "electronVersion": "1.7.5",
    "asar": false,
    "files": [
      "www/**/*",
      "electron/*"
    ]
  },
  "description": "GUI for bisq",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-clipboard": {}
    },
    "platforms": [
      "android"
    ]
  }
}
